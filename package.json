{
	"name": "vsc-saber2pr",
	"displayName": "Saber2pr Tools",
	"description": "saber2pr utils tools.",
	"publisher": "saber2pr",
	"version": "0.0.6",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/Saber2pr/saber2pr.git"
	},
	"icon": "assets/logo.png",
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "saber2pr.open.file.window",
				"title": "%saber2pr.title.open.file.window%"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "saber2pr.open.file.window",
					"group": "1_modification"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "welcome",
					"title": "Saber2pr",
					"icon": "assets/logo.png"
				}
			]
		},
		"views": {
			"welcome": [
				{
					"id": "saber2pr",
					"name": "%saber2pr.title%"
				},
				{
					"id": "extensions",
					"name": "%saber2pr.title.extensions%"
				},
				{
					"id": "system",
					"name": "%saber2pr.title.system%"
				}
			]
		},
		"viewsWelcome": [
			{
				"view": "saber2pr",
				"contents": "%saber2pr.menu%"
			},
			{
				"view": "extensions",
				"contents": "%saber2pr.menu.extensions%"
			},
			{
				"view": "system",
				"contents": "%saber2pr.menu.system%"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"build": "vsce package",
		"release": "standard-version"
	},
	"devDependencies": {
		"@types/git-url-parse": "^9.0.1",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.56.0",
		"standard-version": "^9.3.2",
		"typescript": "^4.1.3"
	},
	"dependencies": {
		"git-url-parse": "^11.6.0",
		"vscode-nls-i18n": "^0.2.4"
	},
	"extensionDependencies": [
		"saber2pr.todolist",
		"saber2pr.scripts-manager"
	],
	"extensionPack": [
		"saber2pr.todolist",
		"saber2pr.scripts-manager",
		"saber2pr.import-transform",
		"saber2pr.ts-ast-viewer",
		"saber2pr.createdirindex",
		"ms-ceintl.vscode-language-pack-zh-hans",
		"mhutchie.git-graph",
		"humao.rest-client",
		"hediet.vscode-drawio",
		"jebbs.plantuml",
		"gruntfuggly.todo-tree",
		"connor.translatex",
		"mikey.vscode-fileheader"
	]
}
